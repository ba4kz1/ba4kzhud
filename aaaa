"local HttpService = game:GetService(\"HttpService\")\r\nlocal Players = game:GetService(\"Players\")\r\nlocal RunService = game:GetService(\"RunService\")\r\nlocal UserInputService = game:GetService(\"UserInputService\")\r\nlocal LocalPlayer = Players.LocalPlayer\r\nlocal Cam = workspace.CurrentCamera\r\nlocal Mouse = LocalPlayer:GetMouse()\r\n\r\n-- Verificar carregamento da biblioteca Rayfield\r\nlocal success, Rayfield = pcall(function()\r\n    return loadstring(game:HttpGet('https://sirius.menu/rayfield'))()\r\nend)\r\n\r\nif not success then\r\n    game:GetService(\"StarterGui\"):SetCore(\"SendNotification\", {\r\n        Title = \"Erro\",\r\n        Text = \"Falha ao carregar Rayfield. Verifique sua conexão.\",\r\n        Duration = 10\r\n    })\r\n    return\r\nend\r\n\r\n-- Variáveis globais\r\nlocal isAuthenticated = false\r\nlocal isPremium = false\r\nlocal isMaster = false\r\n\r\n-- Criação da janela inicial (autenticação)\r\nlocal Window = Rayfield:CreateWindow({\r\n    Name = \"Ba4kz Hub | Universal BR\",\r\n    Icon = \"key\",\r\n    LoadingTitle = \"Carregando Ba4kz Hub\",\r\n    LoadingSubtitle = \"Criador: Ba4kz\",\r\n    ConfigurationSaving = {\r\n        Enabled = true,\r\n        FolderName = \"Ba4kz Hub\",\r\n        FileName = \"Config\"\r\n    },\r\n    KeySystem = false\r\n})\r\n\r\n-- Aba de Autenticação\r\nlocal AuthTab = Window:CreateTab(\"Autenticação\", \"key\")\r\n\r\n-- Função de validação da chave\r\nlocal function validateKey(inputKey)\r\n    if not inputKey or inputKey == \"\" then\r\n        return false, nil, \"Chave vazia\"\r\n    end\r\n\r\n    local success, response = pcall(function()\r\n        local url = \"https://keys-65bcf-default-rtdb.firebaseio.com/keys/\" .. HttpService:UrlEncode(inputKey) .. \".json\"\r\n        local httpResponse = game:HttpGet(url)\r\n        return HttpService:JSONDecode(httpResponse)\r\n    end)\r\n\r\n    if not success then\r\n        warn(\"Key validation failed: \" .. tostring(response))\r\n        return false, nil, \"Erro de conexão\"\r\n    end\r\n\r\n    if response == nil then\r\n        return false, nil, \"Chave não encontrada\"\r\n    end\r\n\r\n    if not response.valid then\r\n        return false, nil, \"Chave inválida\"\r\n    end\r\n\r\n    print(\"Key valid, type: \" .. tostring(response.type))\r\n    return true, response.type, nil\r\nend\r\n\r\n-- Função de autenticação\r\nlocal function authenticate(inputKey)\r\n    print(\"Authenticating with key: \" .. tostring(inputKey))\r\n    if not inputKey or inputKey == \"\" then\r\n        Rayfield:Notify({\r\n            Title = \"Erro\",\r\n            Content = \"Insira uma chave válida.\",\r\n            Duration = 5,\r\n            Image = \"alert\"\r\n        })\r\n        return\r\n    end\r\n\r\n    local isValid, authType, errorMsg = validateKey(inputKey)\r\n    if isValid then\r\n        local validTypes = {\"normal\", \"premium\", \"master\"}\r\n        if not table.find(validTypes, authType) then\r\n            Rayfield:Notify({\r\n                Title = \"Erro\",\r\n                Content = \"Tipo de chave desconhecido: \" .. tostring(authType),\r\n                Duration = 5,\r\n                Image = \"alert\"\r\n            })\r\n            print(\"Unknown key type: \" .. tostring(authType))\r\n            return\r\n        end\r\n\r\n        isAuthenticated = true\r\n        isPremium = authType == \"premium\" or authType == \"master\"\r\n        isMaster = authType == \"master\"\r\n        Rayfield:Notify({\r\n            Title = \"Sucesso\",\r\n            Content = authType == \"master\" and \"Bem-vindo ao Ba4kz Hub Master!\" or\r\n                      authType == \"premium\" and \"Bem-vindo ao Ba4kz Hub Premium!\" or\r\n                      \"Bem-vindo ao Ba4kz Hub!\",\r\n            Duration = 5,\r\n            Image = \"check\"\r\n        })\r\n        print(\"Authentication successful, key type: \" .. authType)\r\n        pcall(setclipboard, \"https://discord.gg/your-discord\")\r\n        pcall(function() Rayfield:Destroy() end)\r\n        loadMainMenu()\r\n    else\r\n        Rayfield:Notify({\r\n            Title = \"Erro\",\r\n            Content = errorMsg or \"Chave inválida.\",\r\n            Duration = 5,\r\n            Image = \"alert\"\r\n        })\r\n        print(\"Authentication failed: \" .. (errorMsg or \"Invalid key\"))\r\n    end\r\nend\r\n\r\n-- Interface de autenticação\r\nAuthTab:CreateButton({\r\n    Name = \"Copiar Link do Discord\",\r\n    Callback = function()\r\n        pcall(setclipboard, \"https://discord.gg/your-discord\")\r\n        Rayfield:Notify({\r\n            Title = \"Link Copiado\",\r\n            Content = \"Visite nosso Discord para obter uma chave!\",\r\n            Duration = 5,\r\n            Image = \"check\"\r\n        })\r\n    end\r\n})\r\n\r\nAuthTab:CreateInput({\r\n    Name = \"Chave\",\r\n    PlaceholderText = \"Digite a chave...\",\r\n    RemoveTextAfterFocusLost = true,\r\n    Callback = function(value)\r\n        print(\"Key entered: \" .. tostring(value))\r\n        authenticate(value)\r\n    end\r\n})\r\n\r\nAuthTab:CreateParagraph({\r\n    Title = \"Autenticação\",\r\n    Content = \"Insira sua chave abaixo. O sistema identificará se é Normal, Premium ou Master.\\n- Normal: Scripts e visuais.\\n- Premium: Aimbot, ESP, triggerbot.\\n- Master: Fly, noclip, ESP avançado e mais!\"\r\n})\r\n\r\n-- Função para carregar o menu principal\r\nfunction loadMainMenu()\r\n    local success, Rayfield = pcall(function()\r\n        return loadstring(game:HttpGet('https://sirius.menu/rayfield'))()\r\n    end)\r\n\r\n    if not success then\r\n        game:GetService(\"StarterGui\"):SetCore(\"SendNotification\", {\r\n            Title = \"Erro\",\r\n            Text = \"Falha ao recarregar Rayfield.\",\r\n            Duration = 10\r\n        })\r\n        return\r\n    end\r\n\r\n    local Window = Rayfield:CreateWindow({\r\n        Name = \"Ba4kz Hub | Universal BR\",\r\n        Icon = \"key\",\r\n        LoadingTitle = \"Carregando Ba4kz Hub\",\r\n        LoadingSubtitle = \"Criador: Ba4kz\",\r\n        ConfigurationSaving = {\r\n            Enabled = true,\r\n            FolderName = \"Ba4kz Hub\",\r\n            FileName = \"Config\"\r\n        },\r\n        KeySystem = false\r\n    })\r\n\r\n    -- Variáveis globais\r\n    local SelectedScriptID = nil\r\n    local AutoExecute = false\r\n    local Scripts = {\r\n        [1] = {Url = \"https://raw.githubusercontent.com/WeshkyPL/WehskyHub/refs/heads/main/WeshkyHubV1.12.lua\", Name = \"Prison Life\"}\r\n    }\r\n    local aimbotHotkey = Enum.KeyCode.E -- Default hotkey\r\n    local aimbotEnabled = false\r\n    local aimParts = {\"Head\"}\r\n    local aimFov = 100\r\n    local predictionStrength = 0.065\r\n    local smoothing = 0.05\r\n    local stickyAimEnabled = false\r\n    local teamCheck = false\r\n    local wallCheck = true\r\n    local healthCheck = false\r\n    local minHealth = 0\r\n    local antiAim = false\r\n    local antiAimAmountX = 0\r\n    local antiAimAmountY = -100\r\n    local antiAimAmountZ = 0\r\n    local antiAimMethod = \"Reset Velo\"\r\n    local randomVeloRange = 100\r\n    local spinBot = false\r\n    local spinBotSpeed = 20\r\n    local circleColor = Color3.fromRGB(255, 0, 0)\r\n    local targetedCircleColor = Color3.fromRGB(0, 255, 0)\r\n    local rainbowFov = false\r\n    local hue = 0\r\n    local rainbowSpeed = 0.005\r\n    local Esp = false\r\n    local Triggerbot = false\r\n    local EspAdvanced = false\r\n    local Wallhack = false\r\n    local NoRecoil = false\r\n    local SpeedHack = false\r\n    local SpeedValue = 16\r\n    local Fly = false\r\n    local Noclip = false\r\n    local FlySpeed = 50\r\n\r\n    -- Funções de salvar e carregar configurações\r\n    local function SaveSettings()\r\n        if not writefile then return end\r\n        local json = HttpService:JSONEncode({\r\n            SelectedScriptID = SelectedScriptID,\r\n            AutoExecute = AutoExecute,\r\n            aimbotHotkey = aimbotHotkey.Name,\r\n            aimbotEnabled = aimbotEnabled,\r\n            aimParts = aimParts,\r\n            aimFov = aimFov,\r\n            predictionStrength = predictionStrength,\r\n            smoothing = smoothing,\r\n            stickyAimEnabled = stickyAimEnabled,\r\n            teamCheck = teamCheck,\r\n            wallCheck = wallCheck,\r\n            healthCheck = healthCheck,\r\n            minHealth = minHealth,\r\n            antiAim = antiAim,\r\n            antiAimAmountX = antiAimAmountX,\r\n            antiAimAmountY = antiAimAmountY,\r\n            antiAimAmountZ = antiAimAmountZ,\r\n            antiAimMethod = antiAimMethod,\r\n            randomVeloRange = randomVeloRange,\r\n            spinBot = spinBot,\r\n            spinBotSpeed = spinBotSpeed,\r\n            circleColor = {circleColor.R, circleColor.G, circleColor.B},\r\n            targetedCircleColor = {targetedCircleColor.R, targetedCircleColor.G, targetedCircleColor.B},\r\n            rainbowFov = rainbowFov,\r\n            rainbowSpeed = rainbowSpeed,\r\n            Esp = Esp,\r\n            Triggerbot = Triggerbot,\r\n            EspAdvanced = EspAdvanced,\r\n            Wallhack = Wallhack,\r\n            NoRecoil = NoRecoil,\r\n            SpeedHack = SpeedHack,\r\n            SpeedValue = SpeedValue,\r\n            Fly = Fly,\r\n            Noclip = Noclip,\r\n            FlySpeed = FlySpeed\r\n        })\r\n        pcall(function() writefile(\"Ba4kzHubConfig.json\", json) end)\r\n    end\r\n\r\n    local function LoadSettings()\r\n        if not isfile or not readfile then return end\r\n        if isfile(\"Ba4kzHubConfig.json\") then\r\n            local json = readfile(\"Ba4kzHubConfig.json\")\r\n            local settings = HttpService:JSONDecode(json)\r\n            SelectedScriptID = settings.SelectedScriptID\r\n            AutoExecute = settings.AutoExecute or false\r\n            aimbotHotkey = settings.aimbotHotkey and Enum.KeyCode[settings.aimbotHotkey] or aimbotHotkey\r\n            aimbotEnabled = settings.aimbotEnabled or false\r\n            aimParts = settings.aimParts or {\"Head\"}\r\n            aimFov = settings.aimFov or 100\r\n            predictionStrength = settings.predictionStrength or 0.065\r\n            smoothing = settings.smoothing or 0.05\r\n            stickyAimEnabled = settings.stickyAimEnabled or false\r\n            teamCheck = settings.teamCheck or false\r\n            wallCheck = settings.wallCheck or true\r\n            healthCheck = settings.healthCheck or false\r\n            minHealth = settings.minHealth or 0\r\n            antiAim = settings.antiAim or false\r\n            antiAimAmountX = settings.antiAimAmountX or 0\r\n            antiAimAmountY = settings.antiAimAmountY or -100\r\n            antiAimAmountZ = settings.antiAimAmountZ or 0\r\n            antiAimMethod = settings.antiAimMethod or \"Reset Velo\"\r\n            randomVeloRange = settings.randomVeloRange or 100\r\n            spinBot = settings.spinBot or false\r\n            spinBotSpeed = settings.spinBotSpeed or 20\r\n            circleColor = settings.circleColor and Color3.new(unpack(settings.circleColor)) or circleColor\r\n            targetedCircleColor = settings.targetedCircleColor and Color3.new(unpack(settings.targetedCircleColor)) or targetedCircleColor\r\n            rainbowFov = settings.rainbowFov or false\r\n            rainbowSpeed = settings.rainbowSpeed or 0.005\r\n            Esp = settings.Esp or false\r\n            Triggerbot = settings.Triggerbot or false\r\n            EspAdvanced = settings.EspAdvanced or false\r\n            Wallhack = settings.Wallhack or false\r\n            NoRecoil = settings.NoRecoil or false\r\n            SpeedHack = settings.SpeedHack or false\r\n            SpeedValue = settings.SpeedValue or 16\r\n            Fly = settings.Fly or false\r\n            Noclip = settings.Noclip or false\r\n            FlySpeed = settings.FlySpeed or 50\r\n        end\r\n    end\r\n\r\n    pcall(LoadSettings)\r\n\r\n    -- Auto-execute\r\n    if AutoExecute and SelectedScriptID then\r\n        local scriptData = Scripts[SelectedScriptID]\r\n        if scriptData then\r\n            task.spawn(function()\r\n                task.wait(3)\r\n                local success, err = pcall(function()\r\n                    loadstring(game:HttpGet(scriptData.Url))()\r\n                end)\r\n                if not success then\r\n                    Rayfield:Notify({\r\n                        Title = \"Erro no Auto-Execute\",\r\n                        Content = tostring(err),\r\n                        Duration = 5,\r\n                        Image = \"alert\"\r\n                    })\r\n                else\r\n                    Rayfield:Notify({\r\n                        Title = \"Auto-Execute\",\r\n                        Content = \"Script \" .. scriptData.Name .. \" executado!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                end\r\n            end)\r\n        end\r\n    end\r\n\r\n    -- Aba Home\r\n    local HomeTab = Window:CreateTab(\"Home\", \"home\")\r\n    HomeTab:CreateParagraph({\r\n        Title = \"Bem-vindo ao Ba4kz Hub!\",\r\n        Content = isMaster and \"Você é um usuário Master! Acesse fly, noclip e mais na aba Combat.\" or\r\n                  isPremium and \"Você é um usuário Premium! Confira aimbot e ESP na aba Combat.\" or\r\n                  \"Considere uma chave Premium ou Master para mais funcionalidades!\"\r\n    })\r\n    HomeTab:CreateButton({\r\n        Name = \"Copiar Link do Discord\",\r\n        Callback = function()\r\n            pcall(setclipboard, \"https://discord.gg/your-discord\")\r\n            Rayfield:Notify({\r\n                Title = \"Discord Copiado\",\r\n                Content = \"Link copiado!\",\r\n                Duration = 5,\r\n                Image = \"check\"\r\n            })\r\n        end\r\n    })\r\n    HomeTab:CreateParagraph({\r\n        Title = \"Dicas\",\r\n        Content = \"- Scripts: Hacks para jogos e o ZARALHO Mod Menu.\\n- Combat (Premium/Master): Aimbot, fly, noclip.\\n- Visuals: Fullbright.\\n- Server: Gerencie servidores.\"\r\n    })\r\n\r\n    -- Aba Scripts\r\n    local ScriptsTab = Window:CreateTab(\"Scripts\", \"play\")\r\n    ScriptsTab:CreateParagraph({\r\n        Title = \"Scripts\",\r\n        Content = \"Execute scripts para jogos ou use scripts fixos. ZARALHO Mod Menu é experimental!\"\r\n    })\r\n\r\n    ScriptsTab:CreateToggle({\r\n        Name = \"Auto-Execute\",\r\n        CurrentValue = AutoExecute,\r\n        Callback = function(value)\r\n            AutoExecute = value\r\n            SaveSettings()\r\n            Rayfield:Notify({\r\n                Title = \"Auto-Execute\",\r\n                Content = value and \"Ativado!\" or \"Desativado!\",\r\n                Duration = 5,\r\n                Image = value and \"play\" or \"pause\"\r\n            })\r\n        end\r\n    })\r\n\r\n    local function AddScriptButton(tab, scriptData)\r\n        tab:CreateButton({\r\n            Name = scriptData.Name,\r\n            Callback = function()\r\n                local success, err = pcall(function()\r\n                    loadstring(game:HttpGet(scriptData.Url))()\r\n                end)\r\n                if not success then\r\n                    Rayfield:Notify({\r\n                        Title = \"Erro\",\r\n                        Content = \"Falha ao carregar \" .. scriptData.Name .. \": \" .. tostring(err),\r\n                        Duration = 5,\r\n                        Image = \"alert\"\r\n                    })\r\n                else\r\n                    SelectedScriptID = scriptData.Id\r\n                    SaveSettings()\r\n                    Rayfield:Notify({\r\n                        Title = \"Script Carregado\",\r\n                        Content = scriptData.Name .. \" executado!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                end\r\n            end\r\n        })\r\n    end\r\n\r\n    for id, data in pairs(Scripts) do\r\n        data.Id = id\r\n        AddScriptButton(ScriptsTab, data)\r\n    end\r\n\r\n    ScriptsTab:CreateSection(\"Scripts Fixos\")\r\n    ScriptsTab:CreateButton({\r\n        Name = \"Infinity Yield\",\r\n        Callback = function()\r\n            pcall(function() loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))() end)\r\n            Rayfield:Notify({\r\n                Title = \"Infinity Yield\",\r\n                Content = \"Executado!\",\r\n                Duration = 5,\r\n                Image = \"check\"\r\n            })\r\n        end\r\n    })\r\n\r\n    ScriptsTab:CreateButton({\r\n        Name = \"Anti-Lag\",\r\n        Callback = function()\r\n            pcall(function() loadstring(game:HttpGet('https://rawscripts.net/raw/Universal-Script-antilag-15462'))() end)\r\n            Rayfield:Notify({\r\n                Title = \"Anti-Lag\",\r\n                Content = \"Texturas removidas!\",\r\n                Duration = 5,\r\n                Image = \"check\"\r\n            })\r\n        end\r\n    })\r\n\r\n    ScriptsTab:CreateButton({\r\n        Name = \"ZARALHO Mod Menu\",\r\n        Callback = function()\r\n            local Players = game:GetService(\"Players\")\r\n            local LocalPlayer = Players.LocalPlayer\r\n            local Workspace = game:GetService(\"Workspace\")\r\n\r\n            local function notify(title, text)\r\n                Rayfield:Notify({\r\n                    Title = title,\r\n                    Content = text,\r\n                    Duration = 5,\r\n                    Image = \"check\"\r\n                })\r\n            end\r\n\r\n            -- ZARALHO Mod Menu\r\n            local zaralhoMenu = Instance.new(\"ScreenGui\")\r\n            zaralhoMenu.Name = \"ZaralhoMenu\"\r\n            zaralhoMenu.Parent = game:GetService(\"CoreGui\")\r\n            zaralhoMenu.ResetOnSpawn = false\r\n\r\n            local frame = Instance.new(\"Frame\")\r\n            frame.Size = UDim2.new(0, 300, 0, 400)\r\n            frame.Position = UDim2.new(0.5, -150, 0.5, -200)\r\n            frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)\r\n            frame.Parent = zaralhoMenu\r\n\r\n            local title = Instance.new(\"TextLabel\")\r\n            title.Size = UDim2.new(1, 0, 0, 50)\r\n            title.BackgroundColor3 = Color3.fromRGB(50, 50, 50)\r\n            title.Text = \"ZARALHO Mod Menu\"\r\n            title.TextColor3 = Color3.fromRGB(255, 255, 255)\r\n            title.TextScaled = true\r\n            title.Parent = frame\r\n\r\n            local function addButton(name, callback)\r\n                local button = Instance.new(\"TextButton\")\r\n                button.Size = UDim2.new(0.9, 0, 0, 40)\r\n                button.Position = UDim2.new(0.05, 0, 0, #frame:GetChildren() * 50)\r\n                button.BackgroundColor3 = Color3.fromRGB(70, 70, 70)\r\n                button.Text = name\r\n                button.TextColor3 = Color3.fromRGB(255, 255, 255)\r\n                button.TextScaled = true\r\n                button.Parent = frame\r\n                button.MouseButton1Click:Connect(function()\r\n                    pcall(callback)\r\n                end)\r\n            end\r\n\r\n            addButton(\"Deletar Baseplate\", function()\r\n                local baseplate = Workspace:FindFirstChild(\"Baseplate\")\r\n                if baseplate then\r\n                    baseplate:Destroy()\r\n                    notify(\"ZARALHO\", \"Baseplate deletada!\")\r\n                else\r\n                    notify(\"ZARALHO\", \"Baseplate não encontrada!\")\r\n                end\r\n            end)\r\n\r\n            addButton(\"TP para Jogador Aleatório\", function()\r\n                local players = Players:GetPlayers()\r\n                local target = players[math.random(1, #players)]\r\n                if target ~= LocalPlayer and target.Character and target.Character:FindFirstChild(\"HumanoidRootPart\") then\r\n                    LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame\r\n                    notify(\"ZARALHO\", \"Teleportado para \" .. target.Name .. \"!\")\r\n                else\r\n                    notify(\"ZARALHO\", \"Nenhum jogador válido encontrado!\")\r\n                end\r\n            end)\r\n\r\n            addButton(\"Kill Todos\", function()\r\n                for _, player in pairs(Players:GetPlayers()) do\r\n                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild(\"Humanoid\") then\r\n                        player.Character.Humanoid.Health = 0\r\n                    end\r\n                end\r\n                notify(\"ZARALHO\", \"Tentativa de matar todos!\")\r\n            end)\r\n\r\n            addButton(\"Spawnar Cubo Maluco\", function()\r\n                local cube = Instance.new(\"Part\")\r\n                cube.Size = Vector3.new(5, 5, 5)\r\n                cube.Position = LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 10, 0)\r\n                cube.BrickColor = BrickColor.Random()\r\n                cube.Parent = Workspace\r\n                notify(\"ZARALHO\", \"Cubo maluco spawnado!\")\r\n            end)\r\n\r\n            notify(\"ZARALHO Mod Menu\", \"Carregado! Use com cuidado, é experimental!\")\r\n        end\r\n    })\r\n\r\n    -- Aba Visuals\r\n    local VisualsTab = Window:CreateTab(\"Visuals\", \"eye\")\r\n    VisualsTab:CreateParagraph({\r\n        Title = \"Visuals\",\r\n        Content = \"Melhore a visibilidade no jogo.\"\r\n    })\r\n\r\n    local Lighting = game:GetService(\"Lighting\")\r\n    local fullbrightEnabled = false\r\n    VisualsTab:CreateToggle({\r\n        Name = \"Fullbright\",\r\n        CurrentValue = false,\r\n        Callback = function(state)\r\n            fullbrightEnabled = state\r\n            if state then\r\n                Lighting.Brightness = 3\r\n                Lighting.GlobalShadows = false\r\n                Lighting.FogEnd = 100000\r\n            else\r\n                Lighting.Brightness = 1\r\n                Lighting.GlobalShadows = true\r\n                Lighting.FogEnd = 1000\r\n            end\r\n            Rayfield:Notify({\r\n                Title = \"Fullbright\",\r\n                Content = state and \"Ativado!\" or \"Desativado!\",\r\n                Duration = 5,\r\n                Image = state and \"sun\" or \"moon\"\r\n            })\r\n        end\r\n    })\r\n\r\n    -- Aba Server\r\n    local ServerTab = Window:CreateTab(\"Server\", \"globe\")\r\n    ServerTab:CreateParagraph({\r\n        Title = \"Servidores\",\r\n        Content = \"Gerencie servidores.\"\r\n    })\r\n\r\n    ServerTab:CreateButton({\r\n        Name = \"Copiar ID do Servidor\",\r\n        Callback = function()\r\n            local serverId = game.JobId\r\n            if setclipboard then\r\n                setclipboard(serverId)\r\n                Rayfield:Notify({\r\n                    Title = \"Sucesso\",\r\n                    Content = \"ID copiado!\",\r\n                    Duration = 5,\r\n                    Image = \"check\"\r\n                })\r\n            else\r\n                Rayfield:Notify({\r\n                    Title = \"Erro\",\r\n                    Content = \"Executor não suporta copiar.\",\r\n                    Duration = 5,\r\n                    Image = \"alert\"\r\n                })\r\n            end\r\n        end\r\n    })\r\n\r\n    local serverIdInput = \"\"\r\n    ServerTab:CreateInput({\r\n        Name = \"ID do Servidor\",\r\n        PlaceholderText = \"Insira o ID...\",\r\n        Callback = function(input)\r\n            serverIdInput = input\r\n        end\r\n    })\r\n\r\n    ServerTab:CreateButton({\r\n        Name = \"Teleportar\",\r\n        Callback = function()\r\n            if serverIdInput and serverIdInput ~= \"\" then\r\n                local TeleportService = game:GetService(\"TeleportService\")\r\n                local placeId = game.PlaceId\r\n                pcall(function()\r\n                    TeleportService:TeleportToPlaceInstance(placeId, serverIdInput, Players.LocalPlayer)\r\n                end)\r\n            else\r\n                Rayfield:Notify({\r\n                    Title = \"Erro\",\r\n                    Content = \"Insira um ID válido!\",\r\n                    Duration = 5,\r\n                    Image = \"alert\"\r\n                })\r\n            end\r\n        end\r\n    })\r\n\r\n    -- Aba Configuração\r\n    local ConfigTab = Window:CreateTab(\"Configuração\", \"settings\")\r\n    ConfigTab:CreateParagraph({\r\n        Title = \"Configurações\",\r\n        Content = \"Gerencie as configurações do Ba4kz Hub, incluindo aimbot e outras funcionalidades.\"\r\n    })\r\n\r\n    ConfigTab:CreateButton({\r\n        Name = \"Salvar Configurações\",\r\n        Callback = function()\r\n            SaveSettings()\r\n            Rayfield:Notify({\r\n                Title = \"Configurações\",\r\n                Content = \"Configurações salvas com sucesso!\",\r\n                Duration = 5,\r\n                Image = \"check\"\r\n            })\r\n        end\r\n    })\r\n\r\n    ConfigTab:CreateButton({\r\n        Name = \"Carregar Configurações\",\r\n        Callback = function()\r\n            LoadSettings()\r\n            Rayfield:Notify({\r\n                Title = \"Configurações\",\r\n                Content = \"Configurações carregadas com sucesso!\",\r\n                Duration = 5,\r\n                Image = \"check\"\r\n            })\r\n        end\r\n    })\r\n\r\n    ConfigTab:CreateButton({\r\n        Name = \"Resetar Configurações\",\r\n        Callback = function()\r\n            if isfile(\"Ba4kzHubConfig.json\") then\r\n                pcall(function() delfile(\"Ba4kzHubConfig.json\") end)\r\n            end\r\n            SelectedScriptID = nil\r\n            AutoExecute = false\r\n            aimbotHotkey = Enum.KeyCode.E\r\n            aimbotEnabled = false\r\n            aimParts = {\"Head\"}\r\n            aimFov = 100\r\n            predictionStrength = 0.065\r\n            smoothing = 0.05\r\n            stickyAimEnabled = false\r\n            teamCheck = false\r\n            wallCheck = true\r\n            healthCheck = false\r\n            minHealth = 0\r\n            antiAim = false\r\n            antiAimAmountX = 0\r\n            antiAimAmountY = -100\r\n            antiAimAmountZ = 0\r\n            antiAimMethod = \"Reset Velo\"\r\n            randomVeloRange = 100\r\n            spinBot = false\r\n            spinBotSpeed = 20\r\n            circleColor = Color3.fromRGB(255, 0, 0)\r\n            targetedCircleColor = Color3.fromRGB(0, 255, 0)\r\n            rainbowFov = false\r\n            rainbowSpeed = 0.005\r\n            Esp = false\r\n            Triggerbot = false\r\n            EspAdvanced = false\r\n            Wallhack = false\r\n            NoRecoil = false\r\n            SpeedHack = false\r\n            SpeedValue = 16\r\n            Fly = false\r\n            Noclip = false\r\n            FlySpeed = 50\r\n            SaveSettings()\r\n            Rayfield:Notify({\r\n                Title = \"Configurações\",\r\n                Content = \"Configurações resetadas para os valores padrão!\",\r\n                Duration = 5,\r\n                Image = \"check\"\r\n            })\r\n        end\r\n    })\r\n\r\n    -- Aba Combat (Premium e Master)\r\n    if isPremium then\r\n        local CombatTab = Window:CreateTab(\"Combat\", \"target\")\r\n        CombatTab:CreateParagraph({\r\n            Title = \"Combat\",\r\n            Content = isMaster and \"Recursos Premium e Master: aimbot, fly, noclip e mais!\" or\r\n                      \"Recursos Premium: aimbot, ESP e triggerbot.\"\r\n        })\r\n\r\n        -- Variáveis do Combat\r\n        local Players = game:GetService(\"Players\")\r\n        local RunService = game:GetService(\"RunService\")\r\n        local UserInputService = game:GetService(\"UserInputService\")\r\n        local LocalPlayer = Players.LocalPlayer\r\n        local Cam = workspace.CurrentCamera\r\n        local Hrp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(\"HumanoidRootPart\")\r\n        local Humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(\"Humanoid\")\r\n        local aiming = false\r\n        local currentTarget = nil\r\n        local currentTargetPart = nil\r\n\r\n        -- Atualizar HRP e Humanoid quando o personagem mudar\r\n        local function onCharacterAdded(character)\r\n            Hrp = character:WaitForChild(\"HumanoidRootPart\")\r\n            Humanoid = character:WaitForChild(\"Humanoid\")\r\n        end\r\n\r\n        LocalPlayer.CharacterAdded:Connect(onCharacterAdded)\r\n        if LocalPlayer.Character then\r\n            onCharacterAdded(LocalPlayer.Character)\r\n        end\r\n\r\n        -- FOV Circle\r\n        local fovCircle = Drawing.new(\"Circle\")\r\n        fovCircle.Thickness = 2\r\n        fovCircle.Radius = aimFov\r\n        fovCircle.Filled = false\r\n        fovCircle.Color = circleColor\r\n        fovCircle.Visible = false\r\n\r\n        -- Fly Logic\r\n        local function startFly()\r\n            local bodyVelocity = Instance.new(\"BodyVelocity\")\r\n            bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)\r\n            bodyVelocity.Velocity = Vector3.new(0, 0, 0)\r\n            bodyVelocity.Parent = LocalPlayer.Character.HumanoidRootPart\r\n\r\n            local bodyGyro = Instance.new(\"BodyGyro\")\r\n            bodyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)\r\n            bodyGyro.P = 10000\r\n            bodyGyro.Parent = LocalPlayer.Character.HumanoidRootPart\r\n\r\n            RunService:BindToRenderStep(\"Fly\", Enum.RenderPriority.Input.Value, function()\r\n                if not Fly then return end\r\n                local cam = Cam.CFrame\r\n                local moveDir = Vector3.new(0, 0, 0)\r\n                if UserInputService:IsKeyDown(Enum.KeyCode.W) then\r\n                    moveDir = moveDir + cam.LookVector\r\n                end\r\n                if UserInputService:IsKeyDown(Enum.KeyCode.S) then\r\n                    moveDir = moveDir - cam.LookVector\r\n                end\r\n                if UserInputService:IsKeyDown(Enum.KeyCode.A) then\r\n                    moveDir = moveDir - cam.RightVector\r\n                end\r\n                if UserInputService:IsKeyDown(Enum.KeyCode.D) then\r\n                    moveDir = moveDir + cam.RightVector\r\n                end\r\n                if UserInputService:IsKeyDown(Enum.KeyCode.Space) then\r\n                    moveDir = moveDir + Vector3.new(0, 1, 0)\r\n                end\r\n                if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl) then\r\n                    moveDir = moveDir - Vector3.new(0, 1, 0)\r\n                end\r\n                bodyVelocity.Velocity = moveDir * FlySpeed\r\n                bodyGyro.CFrame = cam\r\n            end)\r\n        end\r\n\r\n        local function stopFly()\r\n            RunService:UnbindFromRenderStep(\"Fly\")\r\n            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(\"HumanoidRootPart\") then\r\n                for _, obj in pairs(LocalPlayer.Character.HumanoidRootPart:GetChildren()) do\r\n                    if obj:IsA(\"BodyVelocity\") or obj:IsA(\"BodyGyro\") then\r\n                        obj:Destroy()\r\n                    end\r\n                end\r\n            end\r\n        end\r\n\r\n        -- Noclip Logic\r\n        local noclipConnection\r\n        local function startNoclip()\r\n            noclipConnection = RunService.Stepped:Connect(function()\r\n                if not LocalPlayer.Character then return end\r\n                for _, part in pairs(LocalPlayer.Character:GetDescendants()) do\r\n                    if part:IsA(\"BasePart\") then\r\n                        part.CanCollide = false\r\n                    end\r\n                end\r\n            end)\r\n        end\r\n\r\n        local function stopNoclip()\r\n            if noclipConnection then\r\n                noclipConnection:Disconnect()\r\n                noclipConnection = nil\r\n            end\r\n            if LocalPlayer.Character then\r\n                for _, part in pairs(LocalPlayer.Character:GetDescendants()) do\r\n                    if part:IsA(\"BasePart\") then\r\n                        part.CanCollide = true\r\n                    end\r\n                end\r\n            end\r\n        end\r\n\r\n        -- Aimbot Logic\r\n        local function checkTeam(player)\r\n            if teamCheck and player.Team == LocalPlayer.Team then\r\n                return true\r\n            end\r\n            return false\r\n        end\r\n\r\n        local function checkWall(targetCharacter)\r\n            local targetHead = targetCharacter:FindFirstChild(\"Head\")\r\n            if not targetHead then return true end\r\n\r\n            local origin = Cam.CFrame.Position\r\n            local direction = (targetHead.Position - origin).Unit * (targetHead.Position - origin).Magnitude\r\n            local raycastParams = RaycastParams.new()\r\n            raycastParams.FilterDescendantsInstances = {LocalPlayer.Character, targetCharacter}\r\n            raycastParams.FilterType = Enum.RaycastFilterType.Blacklist\r\n\r\n            local raycastResult = workspace:Raycast(origin, direction, raycastParams)\r\n            return raycastResult and raycastResult.Instance ~= nil\r\n        end\r\n\r\n        local function getClosestPart(character)\r\n            local closestPart = nil\r\n            local shortestCursorDistance = aimFov\r\n            local cameraPos = Cam.CFrame.Position\r\n\r\n            for _, partName in ipairs(aimParts) do\r\n                local part = character:FindFirstChild(partName)\r\n                if part then\r\n                    local partPos = Cam:WorldToViewportPoint(part.Position)\r\n                    local screenPos = Vector2.new(partPos.X, partPos.Y)\r\n                    local cursorDistance = (screenPos - Vector2.new(Mouse.X, Mouse.Y)).Magnitude\r\n\r\n                    if cursorDistance \u003C shortestCursorDistance and partPos.Z > 0 then\r\n                        shortestCursorDistance = cursorDistance\r\n                        closestPart = part\r\n                    end\r\n                end\r\n            end\r\n\r\n            return closestPart\r\n        end\r\n\r\n        local function getTarget()\r\n            local nearestPlayer = nil\r\n            local closestPart = nil\r\n            local shortestCursorDistance = aimFov\r\n\r\n            for _, player in ipairs(Players:GetPlayers()) do\r\n                if player ~= LocalPlayer and player.Character and not checkTeam(player) then\r\n                    if player.Character.Humanoid.Health >= minHealth or not healthCheck then\r\n                        local targetPart = getClosestPart(player.Character)\r\n                        if targetPart then\r\n                            local screenPos = Cam:WorldToViewportPoint(targetPart.Position)\r\n                            local cursorDistance = (Vector2.new(screenPos.X, screenPos.Y) - Vector2.new(Mouse.X, Mouse.Y)).Magnitude\r\n\r\n                            if cursorDistance \u003C shortestCursorDistance then\r\n                                if not wallCheck or not checkWall(player.Character) then\r\n                                    shortestCursorDistance = cursorDistance\r\n                                    nearestPlayer = player\r\n                                    closestPart = targetPart\r\n                                end\r\n                            end\r\n                        end\r\n                    end\r\n                end\r\n            end\r\n\r\n            return nearestPlayer, closestPart\r\n        end\r\n\r\n        local function predict(player, part)\r\n            if player and part then\r\n                local velocity = player.Character.HumanoidRootPart.Velocity\r\n                local predictedPosition = part.Position + (velocity * predictionStrength)\r\n                return predictedPosition\r\n            end\r\n            return nil\r\n        end\r\n\r\n        local function smooth(from, to)\r\n            return from:Lerp(to, smoothing)\r\n        end\r\n\r\n        local function aimAt(player, part)\r\n            local predictedPosition = predict(player, part)\r\n            if predictedPosition then\r\n                if player.Character.Humanoid.Health >= minHealth or not healthCheck then\r\n                    local targetCFrame = CFrame.new(Cam.CFrame.Position, predictedPosition)\r\n                    Cam.CFrame = smooth(Cam.CFrame, targetCFrame)\r\n                end\r\n            end\r\n        end\r\n\r\n        -- Configurar hotkey do aimbot\r\n        local function setAimbotHotkey(input)\r\n            if not input or input == \"\" then\r\n                Rayfield:Notify({\r\n                    Title = \"Erro\",\r\n                    Content = \"Insira uma tecla válida.\",\r\n                    Duration = 5,\r\n                    Image = \"alert\"\r\n                })\r\n                return\r\n            end\r\n\r\n            local keyName = input:upper()\r\n            local keyCode = Enum.KeyCode[keyName]\r\n            if keyCode then\r\n                aimbotHotkey = keyCode\r\n                SaveSettings()\r\n                print(\"Aimbot hotkey set to: \" .. keyName)\r\n                Rayfield:Notify({\r\n                    Title = \"Sucesso\",\r\n                    Content = \"Aimbot hotkey ajustado para \" .. keyName .. \"!\",\r\n                    Duration = 5,\r\n                    Image = \"check\"\r\n                })\r\n            else\r\n                print(\"Invalid key: \" .. input)\r\n                Rayfield:Notify({\r\n                    Title = \"Erro\",\r\n                    Content = \"Tecla inválida: \" .. input,\r\n                    Duration = 5,\r\n                    Image = \"alert\"\r\n                })\r\n            end\r\n        end\r\n\r\n        -- Controles Premium\r\n        CombatTab:CreateSection(\"Recursos Premium\")\r\n        CombatTab:CreateToggle({\r\n            Name = \"Aimbot\",\r\n            CurrentValue = aimbotEnabled,\r\n            Callback = function(state)\r\n                aimbotEnabled = state\r\n                fovCircle.Visible = state\r\n                print(\"Aimbot \" .. (state and \"enabled\" or \"disabled\"))\r\n                Rayfield:Notify({\r\n                    Title = \"Aimbot\",\r\n                    Content = state and \"Ativado! Use a hotkey para mirar.\" or \"Desativado!\",\r\n                    Duration = 5,\r\n                    Image = state and \"play\" or \"pause\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateInput({\r\n            Name = \"Aimbot Hotkey\",\r\n            PlaceholderText = \"Enter key (e.g., E, Q, V)...\",\r\n            RemoveTextAfterFocusLost = true,\r\n            Callback = function(value)\r\n                setAimbotHotkey(value)\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateToggle({\r\n            Name = \"Sticky Aim\",\r\n            CurrentValue = stickyAimEnabled,\r\n            Callback = function(state)\r\n                stickyAimEnabled = state\r\n                Rayfield:Notify({\r\n                    Title = \"Sticky Aim\",\r\n                    Content = state and \"Ativado!\" or \"Desativado!\",\r\n                    Duration = 5,\r\n                    Image = state and \"play\" or \"pause\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateToggle({\r\n            Name = \"Team Check\",\r\n            CurrentValue = teamCheck,\r\n            Callback = function(state)\r\n                teamCheck = state\r\n                Rayfield:Notify({\r\n                    Title = \"Team Check\",\r\n                    Content = state and \"Ativado!\" or \"Desativado!\",\r\n                    Duration = 5,\r\n                    Image = state and \"play\" or \"pause\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateToggle({\r\n            Name = \"Wall Check\",\r\n            CurrentValue = wallCheck,\r\n            Callback = function(state)\r\n                wallCheck = state\r\n                Rayfield:Notify({\r\n                    Title = \"Wall Check\",\r\n                    Content = state and \"Ativado!\" or \"Desativado!\",\r\n                    Duration = 5,\r\n                    Image = state and \"play\" or \"pause\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateToggle({\r\n            Name = \"Health Check\",\r\n            CurrentValue = healthCheck,\r\n            Callback = function(state)\r\n                healthCheck = state\r\n                Rayfield:Notify({\r\n                    Title = \"Health Check\",\r\n                    Content = state and \"Ativado!\" or \"Desativado!\",\r\n                    Duration = 5,\r\n                    Image = state and \"play\" or \"pause\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateToggle({\r\n            Name = \"FOV Visibility\",\r\n            CurrentValue = true,\r\n            Callback = function(state)\r\n                if aimbotEnabled then\r\n                    fovCircle.Visible = state\r\n                end\r\n                Rayfield:Notify({\r\n                    Title = \"FOV Visibility\",\r\n                    Content = state and \"Ativado!\" or \"Desativado!\",\r\n                    Duration = 5,\r\n                    Image = state and \"play\" or \"pause\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateToggle({\r\n            Name = \"Rainbow FOV\",\r\n            CurrentValue = rainbowFov,\r\n            Callback = function(state)\r\n                rainbowFov = state\r\n                Rayfield:Notify({\r\n                    Title = \"Rainbow FOV\",\r\n                    Content = state and \"Ativado!\" or \"Desativado!\",\r\n                    Duration = 5,\r\n                    Image = state and \"play\" or \"pause\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateSlider({\r\n            Name = \"FOV\",\r\n            Range = {0, 1000},\r\n            Increment = 1,\r\n            CurrentValue = aimFov,\r\n            Callback = function(value)\r\n                aimFov = value\r\n                fovCircle.Radius = value\r\n                Rayfield:Notify({\r\n                    Title = \"FOV\",\r\n                    Content = \"Ajustado para \" .. value .. \"!\",\r\n                    Duration = 5,\r\n                    Image = \"check\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateSlider({\r\n            Name = \"Smoothing\",\r\n            Range = {0, 100},\r\n            Increment = 1,\r\n            CurrentValue = (1 - smoothing) * 100,\r\n            Callback = function(value)\r\n                smoothing = 1 - (value / 100)\r\n                Rayfield:Notify({\r\n                    Title = \"Smoothing\",\r\n                    Content = \"Ajustado para \" .. value .. \"!\",\r\n                    Duration = 5,\r\n                    Image = \"check\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateSlider({\r\n            Name = \"Prediction Strength\",\r\n            Range = {0, 0.2},\r\n            Increment = 0.001,\r\n            CurrentValue = predictionStrength,\r\n            Callback = function(value)\r\n                predictionStrength = value\r\n                Rayfield:Notify({\r\n                    Title = \"Prediction Strength\",\r\n                    Content = \"Ajustado para \" .. value .. \"!\",\r\n                    Duration = 5,\r\n                    Image = \"check\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateSlider({\r\n            Name = \"Min Health\",\r\n            Range = {0, 100},\r\n            Increment = 1,\r\n            CurrentValue = minHealth,\r\n            Callback = function(value)\r\n                minHealth = value\r\n                Rayfield:Notify({\r\n                    Title = \"Min Health\",\r\n                    Content = \"Ajustado para \" .. value .. \"!\",\r\n                    Duration = 5,\r\n                    Image = \"check\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateDropdown({\r\n            Name = \"Aim Parts\",\r\n            Options = {\"Head\", \"HumanoidRootPart\", \"Left Arm\", \"Right Arm\", \"Torso\", \"Left Leg\", \"Right Leg\"},\r\n            CurrentOption = aimParts,\r\n            MultipleOptions = true,\r\n            Callback = function(options)\r\n                aimParts = options\r\n                Rayfield:Notify({\r\n                    Title = \"Aim Parts\",\r\n                    Content = \"Partes selecionadas: \" .. table.concat(options, \", \") .. \"!\",\r\n                    Duration = 5,\r\n                    Image = \"target\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateColorPicker({\r\n            Name = \"FOV Color\",\r\n            Color = circleColor,\r\n            Callback = function(color)\r\n                circleColor = color\r\n                if not rainbowFov then\r\n                    fovCircle.Color = color\r\n                end\r\n                Rayfield:Notify({\r\n                    Title = \"FOV Color\",\r\n                    Content = \"Cor ajustada!\",\r\n                    Duration = 5,\r\n                    Image = \"check\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateColorPicker({\r\n            Name = \"Targeted FOV Color\",\r\n            Color = targetedCircleColor,\r\n            Callback = function(color)\r\n                targetedCircleColor = color\r\n                Rayfield:Notify({\r\n                    Title = \"Targeted FOV Color\",\r\n                    Content = \"Cor ajustada!\",\r\n                    Duration = 5,\r\n                    Image = \"check\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateToggle({\r\n            Name = \"ESP\",\r\n            CurrentValue = Esp,\r\n            Callback = function(state)\r\n                Esp = state\r\n                if not state then\r\n                    for _, player in pairs(Players:GetPlayers()) do\r\n                        if player.Character then\r\n                            local highlight = player.Character:FindFirstChildOfClass(\"Highlight\")\r\n                            if highlight then highlight:Destroy() end\r\n                        end\r\n                    end\r\n                end\r\n                Rayfield:Notify({\r\n                    Title = \"ESP\",\r\n                    Content = state and \"Ativado!\" or \"Desativado!\",\r\n                    Duration = 5,\r\n                    Image = state and \"eye\" or \"eye-off\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        CombatTab:CreateToggle({\r\n            Name = \"Triggerbot\",\r\n            CurrentValue = Triggerbot,\r\n            Callback = function(state)\r\n                Triggerbot = state\r\n                Rayfield:Notify({\r\n                    Title = \"Triggerbot\",\r\n                    Content = state and \"Ativado!\" or \"Desativado!\",\r\n                    Duration = 5,\r\n                    Image = state and \"play\" or \"pause\"\r\n                })\r\n                SaveSettings()\r\n            end\r\n        })\r\n\r\n        -- Controles Master\r\n        if isMaster then\r\n            CombatTab:CreateSection(\"Recursos Master - Combate\")\r\n            CombatTab:CreateToggle({\r\n                Name = \"Anti-Aim\",\r\n                CurrentValue = antiAim,\r\n                Callback = function(state)\r\n                    antiAim = state\r\n                    print(\"Anti-aim \" .. (state and \"enabled\" or \"disabled\"))\r\n                    Rayfield:Notify({\r\n                        Title = \"Anti-Aim\",\r\n                        Content = state and \"Ativado!\" or \"Desativado!\",\r\n                        Duration = 5,\r\n                        Image = state and \"play\" or \"pause\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateDropdown({\r\n                Name = \"Anti-Aim Method\",\r\n                Options = {\"Reset Velo\", \"Random Velo\", \"Reset Pos [BROKEN]\"},\r\n                CurrentOption = antiAimMethod,\r\n                Callback = function(option)\r\n                    antiAimMethod = type(option) == \"table\" and option[1] or option\r\n                    print(\"Anti-aim method: \" .. antiAimMethod)\r\n                    Rayfield:Notify({\r\n                        Title = \"Anti-Aim Method\",\r\n                        Content = \"Método ajustado para \" .. antiAimMethod .. \"!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateSlider({\r\n                Name = \"Anti-Aim Amount X\",\r\n                Range = {-1000, 1000},\r\n                Increment = 10,\r\n                CurrentValue = antiAimAmountX,\r\n                Callback = function(value)\r\n                    antiAimAmountX = value\r\n                    Rayfield:Notify({\r\n                        Title = \"Anti-Aim Amount X\",\r\n                        Content = \"Ajustado para \" .. value .. \"!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateSlider({\r\n                Name = \"Anti-Aim Amount Y\",\r\n                Range = {-1000, 1000},\r\n                Increment = 10,\r\n                CurrentValue = antiAimAmountY,\r\n                Callback = function(value)\r\n                    antiAimAmountY = value\r\n                    Rayfield:Notify({\r\n                        Title = \"Anti-Aim Amount Y\",\r\n                        Content = \"Ajustado para \" .. value .. \"!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateSlider({\r\n                Name = \"Anti-Aim Amount Z\",\r\n                Range = {-1000, 1000},\r\n                Increment = 10,\r\n                CurrentValue = antiAimAmountZ,\r\n                Callback = function(value)\r\n                    antiAimAmountZ = value\r\n                    Rayfield:Notify({\r\n                        Title = \"Anti-Aim Amount Z\",\r\n                        Content = \"Ajustado para \" .. value .. \"!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateSlider({\r\n                Name = \"Random Velo Range\",\r\n                Range = {0, 1000},\r\n                Increment = 10,\r\n                CurrentValue = randomVeloRange,\r\n                Callback = function(value)\r\n                    randomVeloRange = value\r\n                    Rayfield:Notify({\r\n                        Title = \"Random Velo Range\",\r\n                        Content = \"Ajustado para \" .. value .. \"!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateToggle({\r\n                Name = \"Spin-Bot\",\r\n                CurrentValue = spinBot,\r\n                Callback = function(state)\r\n                    spinBot = state\r\n                    if state then\r\n                        for _, v in pairs(Hrp:GetChildren()) do\r\n                            if v.Name == \"Spinning\" then\r\n                                v:Destroy()\r\n                            end\r\n                        end\r\n                        LocalPlayer.Character.Humanoid.AutoRotate = false\r\n                        local Spin = Instance.new(\"BodyAngularVelocity\")\r\n                        Spin.Name = \"Spinning\"\r\n                        Spin.Parent = Hrp\r\n                        Spin.MaxTorque = Vector3.new(0, math.huge, 0)\r\n                        Spin.AngularVelocity = Vector3.new(0, spinBotSpeed, 0)\r\n                        Rayfield:Notify({\r\n                            Title = \"Spin-Bot\",\r\n                            Content = \"Ativado!\",\r\n                            Duration = 5,\r\n                            Image = \"play\"\r\n                        })\r\n                    else\r\n                        for _, v in pairs(Hrp:GetChildren()) do\r\n                            if v.Name == \"Spinning\" then\r\n                                v:Destroy()\r\n                            end\r\n                        end\r\n                        LocalPlayer.Character.Humanoid.AutoRotate = true\r\n                        Rayfield:Notify({\r\n                            Title = \"Spin-Bot\",\r\n                            Content = \"Desativado!\",\r\n                            Duration = 5,\r\n                            Image = \"pause\"\r\n                        })\r\n                    end\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateSlider({\r\n                Name = \"Spin-Bot Speed\",\r\n                Range = {0, 1000},\r\n                Increment = 1,\r\n                CurrentValue = spinBotSpeed,\r\n                Callback = function(value)\r\n                    spinBotSpeed = value\r\n                    if spinBot then\r\n                        for _, v in pairs(Hrp:GetChildren()) do\r\n                            if v.Name == \"Spinning\" then\r\n                                v:Destroy()\r\n                            end\r\n                        end\r\n                        local Spin = Instance.new(\"BodyAngularVelocity\")\r\n                        Spin.Name = \"Spinning\"\r\n                        Spin.Parent = Hrp\r\n                        Spin.MaxTorque = Vector3.new(0, math.huge, 0)\r\n                        Spin.AngularVelocity = Vector3.new(0, value, 0)\r\n                    end\r\n                    Rayfield:Notify({\r\n                        Title = \"Spin-Bot Speed\",\r\n                        Content = \"Ajustado para \" .. value .. \"!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateToggle({\r\n                Name = \"ESP Avançado\",\r\n                CurrentValue = EspAdvanced,\r\n                Callback = function(state)\r\n                    EspAdvanced = state\r\n                    if not state then\r\n                        for _, player in pairs(Players:GetPlayers()) do\r\n                            if player.Character then\r\n                                local billboard = player.Character:FindFirstChild(\"Ba4kzEspBillboard\")\r\n                                if billboard then billboard:Destroy() end\r\n                            end\r\n                        end\r\n                    end\r\n                    Rayfield:Notify({\r\n                        Title = \"ESP Avançado\",\r\n                        Content = state and \"Ativado!\" or \"Desativado!\",\r\n                        Duration = 5,\r\n                        Image = state and \"eye\" or \"eye-off\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateToggle({\r\n                Name = \"Wallhack\",\r\n                CurrentValue = Wallhack,\r\n                Callback = function(state)\r\n                    Wallhack = state\r\n                    if not state then\r\n                        for _, player in pairs(Players:GetPlayers()) do\r\n                            if player.Character then\r\n                                local highlight = player.Character:FindFirstChildOfClass(\"Highlight\")\r\n                                if highlight then highlight:Destroy() end\r\n                            end\r\n                        end\r\n                    end\r\n                    Rayfield:Notify({\r\n                        Title = \"Wallhack\",\r\n                        Content = state and \"Ativado!\" or \"Desativado!\",\r\n                        Duration = 5,\r\n                        Image = state and \"eye\" or \"eye-off\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateToggle({\r\n                Name = \"No Recoil\",\r\n                CurrentValue = NoRecoil,\r\n                Callback = function(state)\r\n                    NoRecoil = state\r\n                    Rayfield:Notify({\r\n                        Title = \"No Recoil\",\r\n                        Content = state and \"Ativado!\" or \"Desativado!\",\r\n                        Duration = 5,\r\n                        Image = state and \"play\" or \"pause\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateSection(\"Recursos Master - Mobilidade\")\r\n            CombatTab:CreateToggle({\r\n                Name = \"Fly\",\r\n                CurrentValue = Fly,\r\n                Callback = function(state)\r\n                    Fly = state\r\n                    if state then\r\n                        startFly()\r\n                        Rayfield:Notify({\r\n                            Title = \"Fly\",\r\n                            Content = \"Ativado! Use W,A,S,D, Space, Ctrl para mover.\",\r\n                            Duration = 5,\r\n                            Image = \"play\"\r\n                        })\r\n                    else\r\n                        stopFly()\r\n                        Rayfield:Notify({\r\n                            Title = \"Fly\",\r\n                            Content = \"Desativado!\",\r\n                            Duration = 5,\r\n                            Image = \"pause\"\r\n                        })\r\n                    end\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateSlider({\r\n                Name = \"Fly Speed\",\r\n                Range = {10, 200},\r\n                Increment = 10,\r\n                CurrentValue = FlySpeed,\r\n                Callback = function(value)\r\n                    FlySpeed = value\r\n                    Rayfield:Notify({\r\n                        Title = \"Fly Speed\",\r\n                        Content = \"Ajustado para \" .. value .. \"!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateToggle({\r\n                Name = \"Noclip\",\r\n                CurrentValue = Noclip,\r\n                Callback = function(state)\r\n                    Noclip = state\r\n                    if state then\r\n                        startNoclip()\r\n                        Rayfield:Notify({\r\n                            Title = \"Noclip\",\r\n                            Content = \"Ativado! Passe por paredes.\",\r\n                            Duration = 5,\r\n                            Image = \"play\"\r\n                        })\r\n                    else\r\n                        stopNoclip()\r\n                        Rayfield:Notify({\r\n                            Title = \"Noclip\",\r\n                            Content = \"Desativado!\",\r\n                            Duration = 5,\r\n                            Image = \"pause\"\r\n                        })\r\n                    end\r\n                    SaveSettings()\r\n                end\r\n            })\r\n\r\n            CombatTab:CreateSlider({\r\n                Name = \"Speed Hack\",\r\n                Range = {16, 100},\r\n                Increment = 1,\r\n                CurrentValue = SpeedValue,\r\n                Callback = function(value)\r\n                    SpeedValue = value\r\n                    SpeedHack = value > 16\r\n                    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild(\"Humanoid\") then\r\n                        LocalPlayer.Character.Humanoid.WalkSpeed = value\r\n                    end\r\n                    Rayfield:Notify({\r\n                        Title = \"Speed Hack\",\r\n                        Content = \"Velocidade ajustada para \" .. value .. \"!\",\r\n                        Duration = 5,\r\n                        Image = \"check\"\r\n                    })\r\n                    SaveSettings()\r\n                end\r\n            })\r\n        end\r\n\r\n        -- Lógica do Combat\r\n        RunService.RenderStepped:Connect(function()\r\n            if not LocalPlayer.Character or not Cam then return end\r\n\r\n            if aimbotEnabled then\r\n                local offset = 50\r\n                fovCircle.Position = Vector2.new(Mouse.X, Mouse.Y + offset)\r\n\r\n                if rainbowFov then\r\n                    hue = hue + rainbowSpeed\r\n                    if hue > 1 then hue = 0 end\r\n                    fovCircle.Color = Color3.fromHSV(hue, 1, 1)\r\n                else\r\n                    if aiming and currentTarget then\r\n                        fovCircle.Color = targetedCircleColor\r\n                    else\r\n                        fovCircle.Color = circleColor\r\n                    end\r\n                end\r\n\r\n                if aiming then\r\n                    if stickyAimEnabled and currentTarget then\r\n                        local headPos = Cam:WorldToViewportPoint(currentTarget.Character.Head.Position)\r\n                        local screenPos = Vector2.new(headPos.X, headPos.Y)\r\n                        local cursorDistance = (screenPos - Vector2.new(Mouse.X, Mouse.Y)).Magnitude\r\n\r\n                        if cursorDistance > aimFov or (wallCheck and checkWall(currentTarget.Character)) or checkTeam(currentTarget) then\r\n                            currentTarget = nil\r\n                        end\r\n                    end\r\n\r\n                    if not stickyAimEnabled or not currentTarget then\r\n                        local target, targetPart = getTarget()\r\n                        currentTarget = target\r\n                        currentTargetPart = targetPart\r\n                        if target then\r\n                            print(\"Targeting: \" .. target.Name)\r\n                        end\r\n                    end\r\n\r\n                    if currentTarget and currentTargetPart then\r\n                        aimAt(currentTarget, currentTargetPart)\r\n                    end\r\n                else\r\n                    currentTarget = nil\r\n                end\r\n            end\r\n\r\n            if Triggerbot then\r\n                local closest = nil\r\n                local dist = aimFov\r\n                local camSpot = Cam.CFrame.Position\r\n\r\n                for _, player in pairs(Players:GetPlayers()) do\r\n                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild(\"Head\") and player.Character:FindFirstChild(\"Humanoid\") then\r\n                        if player.Character.Humanoid.Health \u003C= 0 then continue end\r\n                        if teamCheck and player.Team == LocalPlayer.Team then continue end\r\n                        if healthCheck and player.Character.Humanoid.Health \u003C minHealth then continue end\r\n\r\n                        local spot = player.Character.Head.Position\r\n                        local screen, visible = Cam:WorldToViewportPoint(spot)\r\n                        local howFar = (Vector2.new(screen.X, screen.Y) - Vector2.new(Cam.ViewportSize.X / 2, Cam.ViewportSize.Y / 2)).Magnitude\r\n\r\n                        if visible and howFar \u003C dist then\r\n                            local start = camSpot\r\n                            local dir = (spot - start).Unit * 500\r\n                            local rayParams = RaycastParams.new()\r\n                            rayParams.FilterDescendantsInstances = {LocalPlayer.Character}\r\n                            rayParams.FilterType = Enum.RaycastFilterType.Blacklist\r\n                            local rayHit = workspace:Raycast(start, dir, rayParams)\r\n\r\n                            if wallCheck and rayHit and not rayHit.Instance:IsDescendantOf(player.Character) then continue end\r\n\r\n                            dist = howFar\r\n                            closest = player\r\n                        end\r\n                    end\r\n                end\r\n\r\n                if closest then\r\n                    pcall(function() mouse1click() end)\r\n                end\r\n            end\r\n\r\n            if Esp then\r\n                for _, player in pairs(Players:GetPlayers()) do\r\n                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild(\"Humanoid\") then\r\n                        local highlight = player.Character:FindFirstChildOfClass(\"Highlight\")\r\n                        if not highlight then\r\n                            highlight = Instance.new(\"Highlight\")\r\n                            highlight.Parent = player.Character\r\n                            highlight.OutlineColor = Color3.fromRGB(255, 255, 255)\r\n                            highlight.FillColor = Color3.fromRGB(255, 0, 0)\r\n                            highlight.FillTransparency = 0.7\r\n                        end\r\n                    end\r\n                end\r\n            end\r\n\r\n            if isMaster and EspAdvanced then\r\n                for _, player in pairs(Players:GetPlayers()) do\r\n                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild(\"Humanoid\") and player.Character:FindFirstChild(\"Head\") then\r\n                        local billboard = player.Character:FindFirstChild(\"Ba4kzEspBillboard\")\r\n                        if not billboard then\r\n                            billboard = Instance.new(\"BillboardGui\")\r\n                            billboard.Name = \"Ba4kzEspBillboard\"\r\n                            billboard.Parent = player.Character\r\n                            billboard.Adornee = player.Character.Head\r\n                            billboard.Size = UDim2.new(0, 100, 0, 50)\r\n                            billboard.StudsOffset = Vector3.new(0, 3, 0)\r\n                            billboard.AlwaysOnTop = true\r\n\r\n                            local nameLabel = Instance.new(\"TextLabel\")\r\n                            nameLabel.Size = UDim2.new(1, 0, 0, 20)\r\n                            nameLabel.BackgroundTransparency = 1\r\n                            nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)\r\n                            nameLabel.Text = player.Name\r\n                            nameLabel.Parent = billboard\r\n\r\n                            local healthLabel = Instance.new(\"TextLabel\")\r\n                            healthLabel.Size = UDim2.new(1, 0, 0, 20)\r\n                            healthLabel.Position = UDim2.new(0, 0, 0, 20)\r\n                            healthLabel.BackgroundTransparency = 1\r\n                            healthLabel.TextColor3 = Color3.fromRGB(0, 255, 0)\r\n                            healthLabel.Parent = billboard\r\n                        end\r\n\r\n                        local health = player.Character.Humanoid.Health\r\n                        local maxHealth = player.Character.Humanoid.MaxHealth\r\n                        billboard:FindFirstChildOfClass(\"TextLabel\", true).Text = player.Name\r\n                        billboard:FindFirstChildOfClass(\"TextLabel\", true, 2).Text = \"HP: \" .. math.floor(health) .. \"/\" .. maxHealth\r\n                    end\r\n                end\r\n            end\r\n\r\n            if isMaster and Wallhack then\r\n                for _, player in pairs(Players:GetPlayers()) do\r\n                    if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild(\"Humanoid\") then\r\n                        local highlight = player.Character:FindFirstChildOfClass(\"Highlight\")\r\n                        if not highlight then\r\n                            highlight = Instance.new(\"Highlight\")\r\n                            highlight.Parent = player.Character\r\n                            highlight.OutlineColor = Color3.fromRGB(255, 255, 0)\r\n                            highlight.FillColor = Color3.fromRGB(255, 165, 0)\r\n                            highlight.FillTransparency = 0.5\r\n                            highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop\r\n                        end\r\n                    end\r\n                end\r\n            end\r\n\r\n            if isMaster and NoRecoil then\r\n                Cam.CFrame = Cam.CFrame * CFrame.Angles(math.rad(math.random(-1, 1) * 0.1), math.rad(math.random(-1, 1) * 0.1), 0)\r\n            end\r\n        end)\r\n\r\n        RunService.Heartbeat:Connect(function()\r\n            if isMaster and antiAim and Hrp then\r\n                if antiAimMethod == \"Reset Velo\" then\r\n                    local vel = Hrp.Velocity\r\n                    Hrp.Velocity = Vector3.new(antiAimAmountX, antiAimAmountY, antiAimAmountZ)\r\n                    RunService.RenderStepped:Wait()\r\n                    Hrp.Velocity = vel\r\n                elseif antiAimMethod == \"Reset Pos [BROKEN]\" then\r\n                    local pos = Hrp.CFrame\r\n                    Hrp.Velocity = Vector3.new(antiAimAmountX, antiAimAmountY, antiAimAmountZ)\r\n                    RunService.RenderStepped:Wait()\r\n                    Hrp.CFrame = pos\r\n                elseif antiAimMethod == \"Random Velo\" then\r\n                    local vel = Hrp.Velocity\r\n                    local a = math.random(-randomVeloRange, randomVeloRange)\r\n                    local s = math.random(-randomVeloRange, randomVeloRange)\r\n                    local d = math.random(-randomVeloRange, randomVeloRange)\r\n                    Hrp.Velocity = Vector3.new(a, s, d)\r\n                    RunService.RenderStepped:Wait()\r\n                    Hrp.Velocity = vel\r\n                end\r\n            end\r\n        end)\r\n\r\n        -- Bind aimbot hotkey\r\n        UserInputService.InputBegan:Connect(function(input, gameProcessed)\r\n            if gameProcessed then return end\r\n            if input.KeyCode == aimbotHotkey and aimbotEnabled then\r\n                aiming = true\r\n                print(\"Aimbot activated with \" .. aimbotHotkey.Name)\r\n            end\r\n        end)\r\n\r\n        UserInputService.InputEnded:Connect(function(input, gameProcessed)\r\n            if gameProcessed then return end\r\n            if input.KeyCode == aimbotHotkey and aimbotEnabled then\r\n                aiming = false\r\n            end\r\n        end)\r\n    end\r\n\r\n    Rayfield:Notify({\r\n        Title = isMaster and \"Usuário Master\" or isPremium and \"Usuário Premium\" or \"Usuário Normal\",\r\n        Content = \"Ba4kz Hub carregado! Discord: https://discord.gg/your-discord\",\r\n        Duration = 5,\r\n        Image = \"check\"\r\n    })\r\nend\r\n\r\nprint(\"Script initialized.\")"